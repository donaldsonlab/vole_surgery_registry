<!DOCTYPE html>
<html>
  <head>
    <title>VoleSurgeries</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <!---Flash--->
    <% flash.each do |type, message| %>
      <%= content_tag :div, message, class: "flash" %>
    <% end %>

    <%= yield %>
  </body>
</html>
